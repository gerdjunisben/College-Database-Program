<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane stylesheets="@styles.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control.SignInController">
   <children>
      <Pane prefHeight="400.0" prefWidth="400.0">
         <children>
            <TextField fx:id="usernameBox" layoutX="124.0" layoutY="116.0" promptText="username" />
            <PasswordField fx:id="passwordBox" layoutX="124.0" layoutY="174.0" promptText="password" />
            <Button fx:id="signInButton" layoutX="86.0" layoutY="242.0" mnemonicParsing="false" onAction="#signInClick" prefHeight="30.0" prefWidth="77.0" text="sign in" />
            <Button fx:id="signUpButton" layoutX="237.0" layoutY="242.0" mnemonicParsing="false" onAction="#signUpClick" prefHeight="30.0" prefWidth="77.0" text="sign up" />
            <Label fx:id="label" alignment="CENTER" layoutX="147.0" layoutY="73.0" prefHeight="26.0" prefWidth="105.0" text="Sign in" />
         </children>
      </Pane>
   </children>
</AnchorPane>
